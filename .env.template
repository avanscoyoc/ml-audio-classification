# Environment variables for ML Audio Classification

# Google Cloud Platform
GCP_PROJECT_ID=your-gcp-project-id
GCS_BUCKET_DATA=dse-staff
GCS_BUCKET_RESULTS=dse-staff
GOOGLE_APPLICATION_CREDENTIALS=path/to/service-account.json

# Application Configuration
LOG_LEVEL=INFO
EXPERIMENT_SEED=42
MAX_WORKERS=4
RESULTS_DIR=./results

# Model Configuration
MODELS=birdnet,perch,vgg,mobilenet,resnet
SPECIES=coyote,bullfrog,human_vocal
TRAINING_SIZES=10,25,50,100,150,200,250,300
CV_FOLDS=5

# Performance Settings
BATCH_SIZE=32
MAX_TRAINING_SIZE=300
AUDIO_SAMPLE_RATE=22050
AUDIO_DURATION=5.0

# Health Check
HEALTH_CHECK_INTERVAL=30
TIMEOUT_SECONDS=3600